{
	"name": "my-svelte-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:clean": "rimraf node_modules/.vite && rimraf .svelte-kit && vite dev --force",
		"dev:fast": "vite dev --force --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "rimraf node_modules/.vite && rimraf .svelte-kit && rimraf build",
		"clean:win": "if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && if exist .svelte-kit rmdir /s /q .svelte-kit",
		"optimize": "npm run clean:win && npm run dev"
	},
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.0",
		"rimraf": "^6.0.1",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.7.2",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.6",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"chart.js": "^4.4.9",
		"nodejs": "^0.0.0",
		"svelte": "^5.0.0",
		"svelte-carousel": "^1.0.25",
		"svelte-intersection-observer": "^1.0.0",
		"swiper": "^11.2.6",
		"tslib": "^2.6.2",
		"vite": "^6.2.6",
		"zod": "^3.25.67"
	}
}
